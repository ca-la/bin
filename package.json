  "name": "cala-api",
  "version": "1.264.0",
  "description": "REST",
  "main": "dist/index.js",
  "author": "CALA <devops@ca.la>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cala/ts-lib": "^0.4.0",
    "@types/http-assert": "^1.3.0",
    "@types/knex": "^0.14.24",
    "@types/koa-compress": "^2.0.8",
    "@types/lodash": "^4.14.112",
    "@types/mime-types": "^2.1.0",
    "@types/node": "10.3.1",
    "@types/node-uuid": "0.0.28",
    "aws-sdk": "^2.302.0",
    "bcrypt": "2.0.1",
    "co-body": "4.2.0",
    "dd-trace": "^0.7.2",
    "form-data": "2.1.2",
    "google-libphonenumber": "2.0.9",
    "knex": "^0.15.2",
    "koa": "^1.6.2",
    "koa-compress": "^1.0.9",
    "koa-multer": "0.0.2",
    "koa-router": "5.4.0",
    "lodash": "^4.17.11",
    "mime-types": "2.1.21",
    "node-fetch": "1.6.3",
    "node-uuid": "1.4.7",
    "pg": "^7.4.3",
    "pg-rethrow": "1.0.1",
    "probe-image-size": "^4.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "2.9.1",
    "zipcodes": "2.1.0"
  },
  "devDependencies": {
    "@cala/bin": "^2.0.0",
    "@types/csv-parse": "^1.1.11",
    "@types/meow": "^5.0.0",
    "@types/sinon": "^5.0.1",
    "@types/tape": "^4.2.32",
    "csv-parse": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-import-resolver-typescript": "^1.0.2",
    "eslint-plugin-import": "^2.13.0",
    "meow": "^5.0.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "sinon": "^6.1.5",
    "tap-difflet": "^0.7.1",
    "tap-spec": "^5.0.0",
    "tap-xunit": "^2.3.0",
    "tape": "4.6.2"
  },
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "clean": "rm -rf ./dist && mkdir -p ./dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "watch:typescript": "tsc --preserveWatchOutput -w -p .",
    "watch:server": "nodemon --delay 2 -w ./dist dist",
    "dev": "env $(cat .env | xargs -d '\\n') npm-run-all build --parallel watch:*",
    "console": "env $(cat .env | xargs -d '\\n') node",
    "prerelease": "npm run test",
    "release": "cala-release",
    "script": "env $(cat .env | xargs -d '\\n') bin/run",
    "prestart": "stat dist > /dev/null",
    "start": "env $(cat .env | xargs -d '\\n') node dist",
    "migration:validate": "env $(cat .env .test.env | xargs -d '\\n') cala-validate-migration",
    "migration:create": "bin/create-migration",
    "migration:run:local": "env $(cat .env | xargs -d '\\n') bin/migrate local && SKIP_BUILD=true npm run migration:run:test",
    "migration:rollback:local": "env $(cat .env | xargs -d '\\n') bin/migrate local rollback && SKIP_BUILD=true npm run migration:run:test rollback",
    "migration:run:test": "env $(cat .env .test.env | xargs -d '\\n') bin/migrate local",
    "migration:run:staging": "env $(cat .env | xargs -d '\\n') bin/migrate staging",
    "migration:run:production": "env $(cat .env | xargs -d '\\n') bin/migrate production",
    "lint": "bin/lint",
    "tt": "env $(cat .env .test.env | xargs -d '\\n') bin/tt",
    "pretest": "npm-run-all build lint",
    "test": "env $(cat .env .test.env | xargs -d '\\n') bin/test",
    "coverage": "env $(cat .env .test.env | xargs -d '\\n') npx nyc tape \"dist/**/*spec.js\" | npx tap-spec"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "**/*spec.*",
      "coverage/**",
      "dist/migrations/**",
      "src/migrations/**",
      "src/test-helpers/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
